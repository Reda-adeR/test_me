
server{
     port 4444
     host 127.0.0.1

     servername reda1
     limit 100000000000000
     root ../..

     location /html-css (
          get on
          post off
          CGI_PHP /fold/itsmecgi
          auto_index on
          #index index.html
          upload_path /itsmeupload
          root ../../Desktop/root/htmlncss
     )

     location / (
          get on
          post on
          delete on
          CGI_PHP /cgi-php
          auto_index on
          upload_path /
          root ../../Desktop/root
     )

     location /dynamic (
          get on
          post on
          delete on
          CGI_PHP /
          auto_index on
          #index index.html
          upload_path /itsmeupload
          root ../../Desktop/root/dynamic_files
     )
     
}


